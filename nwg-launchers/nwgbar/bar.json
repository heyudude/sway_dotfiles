[
  {
    "name": "Lock screen",
    "exec": "swaylock -f -c 000000",
    "icon": "/usr/share/nwg-launchers/nwgbar/images/system-lock-screen.svg"
  },
  {
    "name": "Logout",
    "exec": "swaymsg exit",
    "icon": "/usr/share/nwg-launchers/nwgbar/images/system-log-out.svg"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "/usr/share/nwg-launchers/nwgbar/images/system-reboot.svg"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "/usr/share/nwg-launchers/nwgbar/images/system-shutdown.svg"
  }
]
